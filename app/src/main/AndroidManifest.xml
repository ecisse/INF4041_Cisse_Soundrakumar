<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="org.esiea.cisse_soundrakumar.myapplication"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="11"
        android:targetSdkVersion="16" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="org.esiea.cisse_soundrakumar.myapplication.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity
            android:name="org.esiea.cisse_soundrakumar.myapplication.DisplayListActivity"
            android:label="@string/title_activity_display_list"
            android:parentActivityName="org.esiea.cisse_soundrakumar.myapplication.MainActivity" >
        </activity>

        <activity
            android:name="org.esiea.cisse_soundrakumar.myapplication.DisplayFormActivity"
            android:label="@string/title_activity_display_form"
            android:parentActivityName="org.esiea.cisse_soundrakumar.myapplication.MainActivity" >
        </activity>
    </application>

</manifest>
